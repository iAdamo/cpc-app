{
  "expo": {
    "name": "CompaniesCenter",
    "slug": "companiescenterllc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/locationperm.png",
    "scheme": "companiescenterllc",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "router": {},
      "apiUrl": "",
      "socketUrl": "",
      "socketPath": "",
      "googleMapsApiKey": "",
      "eas": {
        "projectId": "39222a50-3b60-4626-8a9e-1e415b8d499e"
      }
    },
    "splash": {
      "image": "./assets/images/locationperm.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sanuxtech.companiescenterllc",
      "associatedDomains": ["applinks:companiescenterllc.com"],
      "info": {
        "plist": {
          "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby service providers.",
          "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to show nearby service providers.",
          "NSLocationAlwaysUsageDescription": "This app needs access to your location to show nearby service providers."
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/locationperm.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.MANAGE_MEDIA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "edgeToEdgeEnabled": true,
      "package": "com.sanuxtech.companiescenterllc",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "companiescenterllc.com",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/locationperm.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#102343"
        }
      ],
      [
        "expo-media-library",
        {
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-maps",
        {
          "requestLocationPermission": true,
          "locationPermission": "Allow $(PRODUCT_NAME) to use your location"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "locationAlwaysPermission": "Allow $(PRODUCT_NAME) to use your location.",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      "expo-image-picker",
      "expo-video",
      "expo-sqlite",
      "expo-audio",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
